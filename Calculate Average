#include <stdio.h>
double average(int arr[], int size) {
  double sum = 0;
  double avg;
  for (int i = 0; i < size; i++) {
    sum += arr[i];
  }
  avg = sum / size;
  return avg;
}
int main() {
  int numbers[] = {10, 20, 30, 40, 50};
  int n = 5;
double result;
result = average (numbers, n);
printf("The average of the numbers is %f\n", result);
return 0 ;
}
